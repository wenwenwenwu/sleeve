<!--components/fence/index.wxml-->
<view class="container">
	<text class="title">{{model.title}}</text>
	<view class="row-container">
		<block wx:for="{{model.cells}}" wx:key="index">
			<c-cell class="cell" model="{{item}}" row="{{row}}" line="{{index}}"/>
		</block>
	</view>
	<view class="line"></view>
</view>