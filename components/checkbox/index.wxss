/* components/check-box/index.wxss */
.container{
  width: 40rpx;
  height: 40rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
}

.bg-checked{
  background-color: #157658;
}

.bg-unchecked{
  background-color: #DCEBE6;
  border: 2rpx solid #157658;
}

